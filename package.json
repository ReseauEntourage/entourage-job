{
  "name": "linkedout-back",
  "version": "1.15.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "mainWorker": "NODE_ENV=production node -r dotenv/config --expose-gc build/mainWorker.js",
    "dev-mainWorker": "npx babel-node -r dotenv/config --expose-gc src/mainWorker.js",
    "send-recap": "NODE_ENV=production node -r dotenv/config -e 'require(\"./build/jobs/Mail.js\").sendRecapAboutOffers()'",
    "dev-send-recap": "node -r @babel/register -r dotenv/config -e 'require(\"./src/jobs/Mail.js\").sendRecapAboutOffers()'",
    "dev": "npx babel-node -r dotenv/config src/app.js",
    "start": "NODE_ENV=production node -r dotenv/config build/app.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "dev-test": "NODE_ENV=dev-test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "build": "babel src -d build",
    "db-create": "npx sequelize db:create",
    "db-migrate": "npx sequelize db:migrate",
    "heroku-cleanup": "rm -rf src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "precommit": "NODE_ENV=production lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.31.0",
    "@aws-sdk/s3-request-presigner": "^3.31.0",
    "@mailchimp/mailchimp_marketing": "^3.0.71",
    "@sentry/node": "^5.27.3",
    "@sentry/tracing": "^5.27.3",
    "airtable": "^0.11.1",
    "bull": "^3.19.1",
    "cors": "^2.8.5",
    "dd-trace": "^0.29.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-rate-limit": "^5.1.3",
    "express-sslify": "^1.2.0",
    "google-maps-react": "^2.0.6",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.6",
    "node-mailjet": "^3.3.1",
    "pdf-lib": "^1.16.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "puppeteer-core": "^13.0.1",
    "pusher": "^5.0.0",
    "rate-limit-redis": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.3.0",
    "sequelize-paper-trail": "^3.0.1",
    "sharp": "^0.28.3",
    "text-to-svg": "^3.1.5",
    "throng": "^5.0.0",
    "uuid": "^3.4.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.15.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.20.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.4.1",
    "lint-staged": "^11.2.3",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2"
  }
}
