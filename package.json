{
  "name": "linkedout",
  "version": "1.11.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "mainWorker": "NODE_ENV=production node -r dotenv/config --expose-gc build/backend/mainWorker.js",
    "imageWorker": "NODE_ENV=production node -r dotenv/config --expose-gc build/backend/imageWorker.js",
    "send-recap": "NODE_ENV=production node -r dotenv/config -e 'require(\"src/backend/jobs/Mail.js\").sendRecapAboutOffers()'",
    "dev": "npx babel-node -r dotenv/config src/backend/serveur-next.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "dev-test": "NODE_ENV=dev-test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "build": "next build && babel src -d build --ignore src/test,src/pages,src/hooks,src/components,src/lib",
    "db-create": "npx sequelize db:create",
    "db-migrate": "npx sequelize db:migrate",
    "start": "NODE_ENV=production node -r dotenv/config build/backend/serveur-next.js",
    "uikit-install": "cd ./node_modules/uikit && npm i",
    "uikit-update": "cd ./node_modules/uikit/ && git pull && npm i",
    "uikit-compile": "cp -r ./assets/custom/ ./node_modules/uikit/custom/ && cd ./node_modules/uikit && npm run compile && cd ../../ && cp -r ./node_modules/uikit/dist/ ./static/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "precommit": "NODE_ENV=production lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.27.3",
    "@sentry/react": "^5.29.0",
