{
  "name": "linkedout",
  "version": "1.15.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "mainWorker": "NODE_ENV=production node -r dotenv/config --expose-gc build/backend/mainWorker.js",
    "dev-mainWorker": "npx babel-node -r dotenv/config --expose-gc src/backend/mainWorker.js",
    "send-recap": "NODE_ENV=production node -r dotenv/config -e 'require(\"./build/backend/jobs/Mail.js\").sendRecapAboutOffers()'",
    "dev-send-recap": "node -r @babel/register -r dotenv/config -e 'require(\"./src/backend/jobs/Mail.js\").sendRecapAboutOffers()'",
    "dev": "npx babel-node -r dotenv/config src/backend/serveur-next.js",
    "start": "NODE_ENV=production node -r dotenv/config build/backend/serveur-next.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "dev-test": "NODE_ENV=dev-test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "build": "next build && babel src -d build --ignore src/test,src/pages,src/hooks,src/components,src/lib",
    "db-create": "npx sequelize db:create",
    "db-migrate": "npx sequelize db:migrate",
    "uikit-install": "cd ./node_modules/uikit && npm i",
    "uikit-update": "cd ./node_modules/uikit/ && git pull && npm i",
    "uikit-compile": "cp -r ./assets/custom/ ./node_modules/uikit/custom/ && cd ./node_modules/uikit && npm run compile && cd ../../ && cp -r ./node_modules/uikit/dist/ ./public/static/dist",
    "heroku-cleanup": "rm -rf src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "precommit": "NODE_ENV=production lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/s3-request-presigner": "^3.44.0",
    "@mailchimp/mailchimp_marketing": "^3.0.72",
    "@sentry/node": "^6.16.1",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@sentry/webpack-plugin": "^1.18.3",
    "airtable": "^0.11.1",
    "axios": "^0.24.0",
    "bull": "^4.2.0",
    "circular-dependency-plugin": "^5.2.2",
    "cors": "^2.8.5",
    "dd-trace": "^1.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "express-rate-limit": "^5.5.1",
    "express-sslify": "^1.2.0",
    "google-maps-react": "^2.0.6",
    "ioredis": "^4.28.2",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "next": "^12.0.7",
    "next-with-less": "^2.0.4",
