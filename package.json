    "prop-types": "^15.7.2",
    "puppeteer-core": "^13.0.1",
    "pusher": "^5.0.0",
    "pusher-js": "^7.0.3",
    "rate-limit-redis": "^2.1.0",
    "react": "^17.0.2",
    "react-countup": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-image-file-resizer": "^0.4.7",
    "react-modal": "^3.14.4",
    "react-select": "^5.2.1",
    "react-share": "^3.0.1",
    "react-sortable-hoc": "^2.0.0",
    "rxjs": "^7.4.0",
    "sequelize": "^6.12.1",
    "sequelize-cli": "^6.3.0",
    "sequelize-paper-trail": "^3.0.1",
    "sharp": "^0.29.3",
