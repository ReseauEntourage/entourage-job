{
  "name": "linkedout",
  "version": "1.7.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "worker": "node ./backend/worker.js",
    "dev": "nodemon -r dotenv/config ./backend/serveur-next.js",
    "dev-server": "nodemon --watch ./backend/* -r dotenv/config ./backend/serveur-next.js",
    "dev-front": "node -r dotenv/config ./backend/serveur-next.js",
    "dev-debug": "nodemon --inspect --watch ./backend/* -r dotenv/config ./backend/serveur-next.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "dev-test-coverage": "NODE_ENV=dev-test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "dev-test": "NODE_ENV=dev-test npx jest --watch --config=jest.config.js --runInBand",
    "build": "next build",
    "migrate": "sequelize db:migrate",
    "start": "NODE_ENV=production node -r dotenv/config ./backend/serveur-next.js",
    "uikit-install": "cd ./node_modules/uikit && npm i",
    "uikit-update": "cd ./node_modules/uikit/ && git pull && npm i",
    "uikit-compile": "cp -r ./assets/custom/ ./node_modules/uikit/custom/ && cd ./node_modules/uikit && npm run compile && cd ../../ && cp -r ./node_modules/uikit/dist/ ./static/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "precommit": "NODE_ENV=production lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
