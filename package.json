{
  "name": "linkedout-back",
  "version": "1.25.6",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "mainWorker": "NODE_ENV=production node -r dotenv/config --expose-gc build/mainWorker.js",
    "dev-mainWorker": "npx nodemon --exec babel-node -r dotenv/config --expose-gc src/mainWorker.js",
    "refresh-salesforce-data": "NODE_ENV=production node -r dotenv/config -e 'require(\"./build/jobs/Salesforce.js\").refreshSalesforceData()'",
    "dev-refresh-salesforce-data": "NODE_ENV=development node -r @babel/register -r dotenv/config -e 'require(\"./src/jobs/Salesforce.js\").refreshSalesforceData()'",
    "dev": "npx nodemon --exec babel-node -r dotenv/config src/app.js",
    "start": "NODE_ENV=production node -r dotenv/config build/app.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "dev-test": "NODE_ENV=dev-test npx jest --config=jest.config.js --runInBand --collectCoverage --forceExit --detectOpenHandles",
    "lint": "npx eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "build": "babel src -d build",
    "db-create": "npx sequelize db:create",
    "db-migrate": "npx sequelize db:migrate",
    "db-seed": "npx sequelize db:seed:all",
    "heroku-cleanup": "rm -rf src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
